// cSpell Settings
{
  // Version of the setting file.  Always 0.2
  "version": "0.2",
  // language - current active spelling language
  "language": "en, vi",
  "ignorePaths": [
    "node_modules/",
    "dist/",
    "build/",
    "coverage/",
    "logs/",
    "temp/",
    "tmp/",
    "cspell.json",
    "*.config.*",
    "Dockerfile",
    "*.log",
    "*.svg",
